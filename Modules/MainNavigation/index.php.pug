div(is='wps-main-navigation')
  nav
    ul(class='menu')
      if $data('menuItems')
        for item in $data('menuItems')
          li(class=$data($item, 'css_classes'))
            a(href=$data($item, 'href') title=$data($item, 'title') target=$data($item, 'target'))
              = $data($item, 'text')
